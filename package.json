{
	"name": "preferans-engine-js",
	"version": "0.0.3",
	"description": "nodejs engine for preferans",
	"engines": {
		"node": ">=8.9"
	},
	"main": "lib/preferans.js",
	"scripts": {
		"test": "nyc --reporter=lcov --reporter=html mocha",
		"coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
		"sonar": "sonarjs -e .sonarlint/**,node_modules/**,coverage/**,.nyc_output/**,_ref/**"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cope/preferans-engine-js.git"
	},
	"keywords": [
		"engine",
		"preferans",
		"cards"
	],
	"author": "Predrag Stojadinovic",
	"license": "Unlicense",
	"bugs": {
		"url": "https://github.com/cope/preferans-engine-js/issues"
	},
	"homepage": "https://github.com/cope/preferans-engine-js#readme",
	"dependencies": {
		"lodash": "^4.17.10",
		"preferans-deck-js": "^1.2.0",
		"preferans-paper-js": "^1.1.3"
	},
	"devDependencies": {
		"chai": "^4.1.2",
		"coveralls": "^3.0.2",
		"mocha": "^5.2.0",
		"nyc": "^12.0.2",
		"sonarjs": "^1.0.0"
	}
}
